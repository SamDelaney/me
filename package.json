{
  "name": "me",
  "private": true,
  "type": "module",
  "homepage": "https://samdelaney.github.io/me/",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build && pnpm run postbuild",
    "postbuild": "node -e \"const fs = require('fs'); const path = require('path'); const shellIndex = path.join('dist', 'client', '_shell', 'index.html'); const clientIndex = path.join('dist', 'client', 'index.html'); const client404 = path.join('dist', 'client', '404.html'); if (fs.existsSync(shellIndex)) { fs.copyFileSync(shellIndex, clientIndex); fs.copyFileSync(shellIndex, client404); console.log('Copied index.html from _shell to client root and created 404.html'); }\"",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "deploy": "pnpm run build && gh-pages -d dist/client"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "gl-matrix": "^3.4.4",
    "lucide-react": "^0.544.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.0.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}